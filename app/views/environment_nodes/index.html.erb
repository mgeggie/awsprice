<h1>Listing environment_nodes</h1>

<table>
  <tr>
    <th>Environment</th>
    <th>Node type</th>
    <th>Quantity</th>
    <th>S3type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @environment_nodes.each do |environment_node| %>
  <tr>
    <td><%= environment_node.environment %></td>
    <td><%= environment_node.node_type %></td>
    <td><%= environment_node.quantity %></td>
    <td><%= environment_node.ec2type %></td>
    <td><%= link_to 'Show', environment_node %></td>
    <td><%= link_to 'Edit', edit_environment_node_path(environment_node) %></td>
    <td><%= link_to 'Destroy', environment_node, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

